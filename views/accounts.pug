html
 head
    title='Alle accounts'
style
     include ../static/style.css
     include head.pug
body
 h2=  'Accounts'
 table
    tr 
        th ID
        th NAME
        th EMAIL
    -for(var i = 0; i < data.length; i++)
        ul 
            li.accounts 
                a(href='/account/account?id=' + data[i]._id) #{data[i].name}
                p #{data[i].email}